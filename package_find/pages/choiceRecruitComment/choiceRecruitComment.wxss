page {width:100%;height:100%;}
.publishViewClass {width:100%;height:100%;/* background:#f0f0f0;*/}
.type1 {width:100%;height:auto;background:white;}
.type1 .typeClass {width:calc(100% - 30rpx);margin-left:30rpx;height:100rpx;border-bottom:2rpx solid #f0f0f0;}
.type1 .typeClass image {width:50rpx;height:50rpx;margin-top:26rpx;float:left;}
.type1 .typeClass input {width:calc(100% - 40px);height:80rpx;margin-top:10rpx;line-height:80rpx;float:left;font-size:28rpx;display:block;}
.type2 {width:100%;height:auto;background:white;padding-bottom:36rpx;}
.type2 textarea {width:calc(100% - 30px);margin-left:30rpx;margin-top:30rpx;height:100px;float:left;}
.type2 .ImageViewClass {width:100%;height:auto;float:left;}
.type2 .ImageViewClass .addImageViewClass {width:170rpx;height:170rpx;border:1rpx dashed #999;margin-left:40rpx;border-radius:8rpx;float:left;display:flex;flex-direction:column;align-items:center;justify-content:center;}
.type2 .ImageViewClass .addImageViewClass image {width:60rpx;height:60rpx;}
.type2 .ImageViewClass .addImageViewClass text {display:block;float:left;width:100%;height:40rpx;line-height:40rpx;text-align:center;font-size:24rpx;color:#999;}
.type2 .ImageViewClass .addImageView1Class {margin:0 40rpx;float:left;position:relative;}
.image1 {width:170rpx;height:170rpx;}
.type2 .ImageViewClass .addImageView1Class .image2 {width:40rpx;height:40rpx;position:absolute;top:0px;right:0px;background:white;border-radius:50%;}
.sendViewClass {position:fixed;bottom:0px;height:90rpx;width:100%;background:#f60;display:block;float:left;text-align:center;line-height:90rpx;color:white;font-size:30rpx;border-radius:0;}
.sendViewClass::after {content:none;}
