/*对话框 */

.commodity_attr_box {
  height: 300rpx;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
  padding-top: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.share_container {
  width: 540rpx;
  display: flex;
  justify-content: space-around;
  padding-top: 50rpx;
}

.share_box2 {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.share_words {
  font-size: 30rpx;
}

.words_1 {
  padding-top: 30rpx;
}

.icon_pic {
  width: 60rpx;
  height: 60rpx;
}

/* 改版提问板块 */
.question_box {
  width: 100%;
  display: flex;
  background: #FFFFFF;
  margin-bottom: 20rpx;
}

.user_photo {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 20rpx 0 0 30rpx;
}

.cont_box {
  width: 100%;
  display: flex;
  flex-direction: column;
  margin: 30rpx 20rpx 12rpx 14rpx;
}

.name_time {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.comment_name {
  font-size: 30rpx;
  max-width: 300rpx;
}

.ques_time {
  font-size: 22rpx;
  color: #C7C7C7;
}

.ques_answer_box {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

.ques_cont {
  display: flex;
  flex-direction: column;
  width: 450rpx;
}

.ques_words {
  font-size: 26rpx;
  color: #666666;
  padding: 16rpx 0;
}

.pic_box {
  display: flex;
}

.ques_pic {
  width: 145rpx;
  height: 150rpx;
  margin-right: 5rpx;
  vertical-align: top;
}

.answer_btn {
  width: 105rpx;
  height: 50rpx;
  font-size: 22rpx;
  border-radius: 25rpx;
  background: #1EAC58;
  color: #FFFFFF;
  line-height: 50rpx;
  text-align: center;
}

.answer_box {
  display: flex;
  align-items: center;
  margin-top: 14rpx;
}

.answer_num_box {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  border: 2rpx solid #1EAC58;
  display: flex;
  justify-content: center;
  align-items: center;
}

.answer_num {
  width: 42rpx;
  height: 42rpx;
  background: #1eac58;
  border-radius: 50%;
  font-size: 20rpx;
  color: #FFFFFF;
  display: flex;
  justify-content: center;
  align-items: center;
}

.answer_words {
  font-size: 26rpx;
  color: #868686;
  padding-left: 8rpx;
}

.interactive_box {
  width: 100%;
  height: 8vh;
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0;
  z-index: 10;
}

.send_ques {
  width: 700rpx;
  height: 80rpx;
  border-radius: 40rpx;
  background: #1EAC58;
  color: #FFFFFF;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
