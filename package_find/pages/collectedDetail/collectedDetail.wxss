@import "../../../wxParse/wxParse.wxss";

.scroll_box {
  width: 100%;
  height: 92vh;
  background: #fff;
}

.top_pic_bg {
  width: 750rpx;
  height: 340rpx;
  display: block;
}

.join_detail {
  background: #fff;
}

.blank {
  width: 100%;
  height: 15rpx;
  background: #eaeaea;
}

.name_attend {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 30rpx;
  padding: 30rpx 0;
  border-bottom: 2rpx solid #e9eaec;
}

.book_theme {
  font-size: 34rpx;
  font-weight: bold;
  width: 560rpx;
}

.joinBtn {
  width: 120rpx;
  height: 50rpx;
  background: #ff4e5c;
  color: #fff;
  font-size: 28rpx;
  border-radius: 10rpx;
  text-align: center;
  line-height: 50rpx;
  margin-right: 30rpx;
}

.icon_box {
  display: flex;
  align-items: center;
  margin-left: 30rpx;
  padding: 30rpx 0;
  border-bottom: 2rpx solid #e9eaec;
}

.timeIcon, .placeIcon, .limitedIcon, .countdownIcon {
  font-size: 50rpx;
  color: #333;
  padding-right: 24rpx;
}

.icon_content {
  font-size: 28rpx;
  color: #333;
}

.placeLen {
  max-width: 620rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.countdown_number {
  font-size: 24rpx;
  color: #fff;
  width: 24rpx;
  height: 36rpx;
  background: #333;
  text-align: center;
  line-height: 36rpx;
  border-radius: 4rpx;
  margin-right: 4rpx;
}

.countdown_string {
  font-size: 24rpx;
  color: #333;
}

.line_box {
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.left_line, .right_line {
  width: 255rpx;
  height: 47rpx;
}

.line_words {
  color: #1eac58;
  font-size: 30rpx;
  padding: 0 16rpx;
}

.participants_box {
  width: 500rpx;
  height: 90rpx;
  background: linear-gradient (to right, white, green);
}

.participants {
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
}

.join_num_box {
  width: 720rpx;
  margin: 20rpx 0 0 30rpx;
  border: 2rpx solid #65c76b;
  border-left: none;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background: linear-gradient(to right, #fff, #e5f2e3);
}

.join_img_box {
  padding: 10rpx 0;
  max-width: 550rpx;
  height: 100%;
  overflow: hidden;
}

.join_member_avatar {
  margin: 8rpx 18rpx;
  width: 76rpx;
  height: 76rpx;
  border-radius: 50%;
}

.join_text_box {
  display: flex;
  height: 100%;
}

.text_elli {
  margin-right: 10rpx;
  display: flex;
  align-items: center;
  font-size: 40rpx;
  color: #65c76b;
}

.join_num_text {
  width: 168rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #fff;
  background: #65c76b;
}

.tab_control {
  display: flex;
  justify-content: space-between;
  width: 720rpx;
  height: 80rpx;
  padding-left: 30rpx;
  border-bottom: 2rpx solid #f0f0f0;
}

.tab_control text {
  font-size: 28rpx;
  padding-top: 25rpx;
}

.click_style {
  color: #1eac58;
  border-bottom: 2rpx solid #1eac58;
}

.green_dot {
  width: 8rpx;
  height: 8rpx;
  background: #1eac58;
  border-radius: 50%;
  margin-top: 38rpx;
}

.activ_rule_box {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.introIcon_box {
  display: flex;
  align-items: center;
  margin: 25rpx 0;
}

.introduceIcon {
  font-size: 46rpx;
  color: #1eac58;
}

.iconWords {
  font-size: 34rpx;
  color: #1eac58;
}

.activ_rule {
  width: 690rpx;
}

.green_dot2 {
  width: 15rpx;
  height: 15rpx;
  background: #1eac58;
  border-radius: 50%;
  margin: 14rpx 25rpx 0 0;
}

.rule_words {
  font-size: 28rpx;
  color: #666;
  max-width: 640rpx;
  height: auto;
}

.dot_words_box img {
  width: 100vw !important;
}

/* .rule_pic {
  width: 690rpx;
