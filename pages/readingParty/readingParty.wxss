/* pages/myRelayRead/myRelayRead.wxss */
page,.page,.page__bd {height:100%;width:100%;background:#f0f0f0;}
.page__bd {padding-bottom:0;}
.weui-tab__content {float:left;height:calc(100% - 47px);width:100%;}
.weui-navbar {display:-webkit-box;display:-webkit-flex;display:flex;position:fixed;z-index:5;top:0;width:100%;float:left;}
.weui-tab {position:relative;height:100%;}
.weui-navbar__title {display:inline-block;font-size:14px;max-width:8em;width:auto;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;word-wrap:normal;color:#333;}
.weui-navbar__item {position:relative;display:block;-webkit-box-flex:1;-webkit-flex:1;flex:1;padding:13px 0;text-align:center;font-size:0;background:white;float:left;}
.weui-navbar__item.weui-bar__item_on {color:#1aad19;}
.weui-navbar__slider {position:absolute;content:" ";left:0;bottom:0;width:50%;height:3px;background-color:#1aad19;-webkit-transition:-webkit-transform .3s;transition:-webkit-transform .3s;transition:transform .3s;transition:transform .3s,-webkit-transform .3s;}
.scrollClass {width:100%;margin-top:52px;margin-bottom:50px;}
.dataViewClass {width:100%;height:auto;margin-top:5px;background:white;}
.selectViewClass {width:calc(100% - 25px);height:36px;border-bottom:1px solid #f0f0f0;margin-left:15px;}
.selectViewClass .selectImage {width:20px;height:20px;margin-top:8px;float:left;}
.selectViewClass .text1 {display:block;float:left;margin-left:5px;line-height:36px;font-size:14px;color:#333;}
.selectViewClass .text2 {display:block;float:right;margin-left:5px;line-height:36px;font-size:13px;color:#666;}
.dataViewClass .imageClass {width:80px;height:80px;float:left;margin:15px;}
.dataViewClass .view1 {width:calc(100% - 110px);float:left;}
.dataViewClass .view1 .text1 {display:block;float:left;font-size:13px;color:#333;width:100%;margin-top:15px;line-height:20px;}
.dataViewClass .view1 .text2 {display:block;float:left;font-size:13px;color:#333;width:100%;line-height:20px;}
.dataViewClass .view1 .text3 {display:block;float:left;font-size:12px;color:#666;width:calc(100% - 10px);margin-right:10px;line-height:20px;height:40px;}
.dataViewClass .view2 {width:60px;float:left;margin-left:10px;}
.dataViewClass .view2 .text2 {display:block;float:left;font-size:11px;color:white;width:100%;line-height:25px;text-align:center;border:1px solid #eaeaea;margin-top:5px;background:#aaa;}
.bottomViewClass {width:100%;position:fixed;bottom:0px;height:40px;background:white;}
.bottomViewClass .checkClass {width:30%;height:100%;float:left;}
.bottomViewClass .checkClass image {width:20px;height:20px;margin-top:10px;float:left;margin-left:15px;}
.bottomViewClass .checkClass text {display:block;float:left;font-size:13px;color:#666;line-height:40px;margin-left:5px;}
.bottomViewClass .text1 {width:calc(35% - 1px);height:100%;border-left:1px solid #f0f0f0;display:block;float:left;text-align:center;line-height:40px;font-size:14px;color:#666;}
.bottomViewClass .text3 {width:calc(35% - 1px);height:100%;border-left:1px solid #f0f0f0;display:block;float:left;text-align:center;line-height:40px;font-size:14px;background:#1aad19;color:white;}
.bottomViewClass .text4 {width:calc(35% - 1px);height:100%;border-left:1px solid #f0f0f0;display:block;float:left;text-align:center;line-height:40px;font-size:14px;background:red;color:white;}
.scroll1Class {width:100%;height:auto;margin-top:52px;background:white;}
.dataView1Class {width:calc(100% - 15px);height:auto;margin-left:15px;border-bottom:1px solid #f0f0f0;}
.dataView1Class .image1Class {width:70px;height:70px;margin:10px 10px 10px 0px;float:left;}
.dataView1Class .view3 {width:calc(100% - 80px);float:left;position:relative;height:90px;}
.dataView1Class .view3 .text1 {display:block;float:left;height:30px;font-size:14px;line-height:30px;margin-top:10px;color:#333;width:100%;}
.dataView1Class .view3 .text2 {display:block;float:left;height:20px;font-size:13px;line-height:20px;color:#666;width:100%;}
.dataView1Class .view3 .text3 {display:block;float:left;height:20px;font-size:13px;line-height:20px;color:#666;width:100%;}
.dataView1Class .view3 .clear {position:absolute;top:10px;right:10px;display:block;float:left;background:red;border-radius:3px;width:40px;line-height:26px;text-align:center;font-size:13px;color:white;}
.bottomView1Class {width:100%;height:44px;line-height:44px;text-align:center;font-size:14px;color:white;background:#ff6600;position:fixed;bottom:0px;}
.touch-item {font-size:14px;display:flex;justify-content:space-between;width:100%;overflow:hidden;background:white;border-top:1px solid #f0f0f0;}
.content {width:100%;margin-right:0;-webkit-transition:all 0.4s;transition:all 0.4s;-webkit-transform:translateX(80px);transform:translateX(80px);margin-left:-80px;}
.del {background-color:orangered;width:80px;display:flex;flex-direction:column;align-items:center;justify-content:center;color:#fff;-webkit-transform:translateX(80px);transform:translateX(80px);-webkit-transition:all 0.4s;transition:all 0.4s;}
.touch-move-active .content,.touch-move-active .del {-webkit-transform:translateX(0);transform:translateX(0);}
.page1 {position:fixed;top:0px;left:0px;width:100%;height:100%;background:white;z-index:10;}
.hint_text {width:60%;margin-left:20%;display:block;float:left;margin-top:50%;font-size:13px;color:#666;line-height:20px;text-align:center;}
.create_read {position:absolute;bottom:50px;background:#1eac58;border-radius:15px;height:40px;color:white;font-size:15px;line-height:40px;width:120px;text-align:center;margin-left:calc(50% - 60px);}
.readyClub {width:90%;margin-left:5%;top:25%;position:absolute;z-index:20;background:white;border-radius:5px;}
.address {width:calc(100% - 20px);margin-left:10px;float:left;padding-top:20px;height:50px;}
.address image {width:18px;height:18px;margin-right:15px;margin-top:16px;position:absolute;right:10px;}
.address text {width:calc(100% - 15px);height:40px;border-radius:5px;border:1px solid #999;padding-left:5px;font-size:14px;line-height:40px;margin-top:4px;display:block;float:left;margin-left:5px;color:#999;}
.create_z {background:#1eac58;border-radius:15px;height:40px;color:white;font-size:15px;line-height:40px;width:120px;text-align:center;margin-left:calc(50% - 60px);margin-bottom:20px;float:left;margin-top:20px;}
