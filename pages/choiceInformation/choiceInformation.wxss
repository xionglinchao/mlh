@import "../../wxParse/wxParse.wxss";
page {width:100%;height:100%;}
.item1_view {width:100%;float:left;background:white;border-bottom:1px solid #f0f0f0;margin-bottom:65px;}
.item1_title {width:100%;height:40px;float:left;}
.item1_title_desc {display:block;float:left;width:60px;color:#1aad19;border:1px solid #1aad19;border-radius:15px;height:24px;margin-top:7px;text-align:center;margin-left:15px;font-size:13px;line-height:24px;}
.item1_title_name {display:block;float:left;width:calc(100% - 87px);margin-left:10px;font-size:14px;color:#333;line-height:40px;}
.item1_image {width:calc(100% - 30px);margin-left:15px;float:left;}
.item1_time {width:100%;height:30px;float:left;border-bottom:1px solid #f0f0f0;}
.item1_time text {line-height:30px;float:left;display:block;color:#999;font-size:12px;margin-left:15px;}
.item1_time image {width:18px;height:18px;margin-top:6px;float:right;}
.html_view {width:calc(100% - 30px);margin-left:15px;float:left;padding:5px 0px;}
.bottom_view {width:100%;height:60px;background:white;position:fixed;bottom:0px;}
.bottom_submit {width:90%;margin-left:5%;background:#1eac58;padding-left:0px;padding-right:0px;height:40px;margin-top:10px;font-size:14px;color:white;line-height:40px;}
.bottom_submit::before {content:none;}
.item1_scroll {width:100%;float:left;}
.item1_scroll_item {width:calc(100% - 15px);margin-left:15px;float:left;border-bottom:1px solid #f0f0f0;padding-bottom:3px;}
.comment_user_view {width:100%;height:40px;float:left;padding-top:5px;}
.user_image {width:30px;height:30px;float:left;margin-right:15px;border-radius:50%;margin-top:5px;}
.user_name_text {display:block;float:left;width:calc(100% - 100px);line-height:40px;font-size:14px;color:#333;}
.go_bottom_image {width:18px;height:18px;margin-right:10px;padding-top:11px;float:right;padding-bottom:11px;padding-left:5px;padding-right:5px;}
.item_list_1 {width:100%;font-size:13px;line-height:22px;color:#666;float:left;display:block;}
.comment_bottom_view {width:100%;height:40px;float:left}
.comment_bottom_time_text {display:block;float:left;line-height:40px;font-size:13px;color:#999;}
.comment_bottom_fx_image {width:24px;height:24px;float:right;margin-top:8px;margin-right:2px;}
.comment_bottom_fx_text {display:block;float:right;font-size:13px;color:#666;line-height:40px;margin-right:15px;}
.comment_bottom_pl_text {display:block;float:right;font-size:13px;color:#666;line-height:40px;margin-right:10px;}
.comment_bottom_pl_image {width:20px;height:20px;float:right;margin-top:10px;margin-right:4px;}
.comment_bottom_zf_image {width:24px;height:24px;float:right;margin-top:8px;margin-right:10px}
.imagesDetails3 {width:calc(33% - 10px);margin-top:5px;margin-right:8px;}
