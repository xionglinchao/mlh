.t-box {width:100%;height:100vh;}
.scroll-box {width:100%;height:92vh;}
.yuyin_title {margin-left:30rpx;padding:35rpx 30rpx 30rpx 0;border-bottom:2rpx solid #f5f5f5;font-size:36rpx;}
.yuyin_cont {margin-left:30rpx;padding-top:30rpx;width:690rpx;height:250rpx;font-size:32rpx;}
.yuyin-box {display:flex;width:720rpx;margin-left:15rpx;}
.add-yuyin {display:flex;flex-direction:column;align-items:center;justify-content:center;width:235rpx;height:235rpx;background-color:#fafafa;border:2rpx dashed #999;margin-right:8rpx;}
.camera {font-size:80rpx;color:#aeaeae;}
.yuyin_words {font-size:22rpx;color:#ccc;}
.yuyin_note {font-size:22rpx;color:#999;padding:19rpx 0 0 50rpx;}
.submit {width:100%;height:100rpx;position:fixed;bottom:0;display:flex;justify-content:center;align-items:center;color:#fefefe;background-color:#eb810e;}
.tempVideo {width:238rpx;height:238rpx;}
.del_img {width:20px;height:20px;position:absolute;top:2px;right:2px;background:white;border-radius:50%;z-index:2;}
