.view1 .text4 {display:block;float:left;font-size:13px;color:#999;margin-left:5px;}
.view1 .text5 {display:block;float:left;font-size:13px;color:#999;margin-left:5px;}
.button1 {width:100%;height:45px;font-size:14px;color:white;background:#ff6600;line-height:45px;padding:0px;border-radius:0px;position:fixed;bottom:0px;}
