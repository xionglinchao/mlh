/* pages/customService/customService.wxss */
page {width:100%;height:100%;}
.pViewClass {width:100%;height:100%;background:#f0f0f0;}
.headClass {width:100%;height:auto;}
.type1 {width:100%;height:auto;background:white;}
.type1 .typeClass {width:calc(100% - 15px);margin-left:15px;height:50px;border-bottom:1px solid #f0f0f0;}
.type1 .typeClass::after {content:none;}
.type1 .typeClass image {width:30px;height:30px;margin-top:10px;float:left;}
.type1 .typeClass text {display:block;float:left;line-height:50px;font-size:12px;color:#999;margin-left:10px;}
