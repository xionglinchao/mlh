/* @import "pages/template/mdk_temp.wxss"; */
@import 'templates/loadView/loadView.wxss';
.container {height:100%;display:flex;flex-direction:column;align-items:center;justify-content:space-between;padding:200rpx 0;box-sizing:border-box;}
/* 对话框 */.comment_box {margin:30rpx;width:690rpx;}
.dialog {padding:20rpx;background:#f8f8f8;display:flex;flex-direction:column;/* align-items:center;*/  position:relative;}
.dialog::before {content:"";width:0;height:0;position:absolute;left:32rpx;top:-28rpx;border-left:6px solid transparent;border-right:6px solid transparent;border-bottom:14px solid #f8f8f8;}
.dialog text {font-size:28rpx;color:#666;word-wrap:break-word;line-height:1.6;}
.dialog_image {margin-top:10rpx;width:100%;height:294rpx;}
.dialog_video {margin-top:10rpx;width:100%;height:300rpx;background:#000;/* display:flex;justify-content:center;align-items:center;*/}
.dialog_video_play {width:68rpx;height:68rpx;position:relative;top:-180rpx;left:296rpx;}
/* gif图加载 */.LoadingViewClass {width:100%;height:100%;position:fixed;top:0px;left:0px;background:white;}
.LoadingViewClass image {width:60px;height:60px;margin-left:calc(50% - 30px);margin-top:50%;z-index:50;}
.ShowToastClass {position:fixed;width:140px;background:#000;top:calc(50% - 70px);left:calc(50% - 70px);opacity:0.8;border-radius:4px;z-index:25;padding-bottom:10px;}
.ShowToastClass image {width:40px;height:40px;margin-left:50px;margin-top:20px;float:left;}
.ShowToastClass text {display:block;float:left;width:100%;color:white;text-align:center;line-height:20px;font-size:13px;margin-top:10px;}
.modelView {width:80%;background:white;position:absolute;top:30%;margin-left:10%;z-index:20;}
.modelView .modelTitle {width:100%;height:40px;display:block;float:left;margin-top:10px;text-align:center;line-height:40px;font-size:16px;color:#333;}
.modelView .modelContent {width:100%;float:left;height:40px;line-height:40px;}
.modelView .modelContent text {display:block;float:left;width:100%;text-align:center;color:#999;font-size:12px;}
.modelView .buttonView {width:100%;height:40px;float:left;margin-top:10px;}
.modelView .buttonView .cancelClass {display:block;float:left;width:calc(50% - 1px);height:100%;color:#939393;text-align:center;line-height:40px;font-size:15px;}
.modelView .buttonView .phoneClass {display:block;float:left;width:calc(50% - 1px);height:100%;color:#19ae19;text-align:center;line-height:40px;font-size:15px;}
/*点击购物车之后遮罩层*/.commodityScreenClass {width:100%;height:100%;position:fixed;background:#000;opacity:0.4;overflow:hidden;z-index:10;color:#fff;top:0px;right:0px;bottom:0px;left:0px;}
/*空数据展示样式*/.gapViewClass {width:100%;height:100%;}
.gapViewClass image {width:100px;height:100px;margin-left:calc(50% - 50px);margin-top:50%;float:left;}
.gapViewClass text {width:100%;height:30px;font-size:14px;color:#666;text-align:center;line-height:30px;display:block;float:left;margin-top:10px;}
.gapViewClass button {float:left;padding-left:0px;padding-right:0px;line-height:30px;border-radius:4px;font-size:14px;width:80px;text-align:center;background:white;border:1px solid #f0f0f0;margin-left:calc(50% - 40px);}
.gapViewClass button::after {content:none;}
/*白色框点击效果*/.bottom_v {background:#f0f0f0;}
.bottom_v_1 {background:#eaeaea;}
.page_view {width:100%;height:100%;}
.text_block {display:block;float:left;}
/*单行超出省略*/.beyondEllipsis {overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
/*第二行超出省略*/.beyondEllipsis_two {display:-webkit-box;-webkit-box-orient:vertical;-webkit-line-clamp:2;overflow:hidden;}
/*音频的播放效果*/@keyframes run {0% {background-position:0 0;}
100% {background-position:-180 0;}
}
@-webkit-keyframes run {0% {background-position:0 0;}
100% {background-position:-120px 0;}
}
@keyframes run {0% {background-position:0 0;}
100% {background-position:-120px 0;}
}
