/* pages/interactiveMessage/interactiveMessage.wxss */
page {width:1005;height:100%;background:white;}
.volunteer_message_scroll {width:100%;height:100%;}
.volunteer_item_view {width:100%;height:60px;float:left;border-bottom:1px solid #f0f0f0;background:white;position:relative;}
.item_image {width:40px;height:40px;float:left;margin-left:10px;padding:10px 0px;}
.item_name {display:block;float:left;line-height:60px;color:#1eac58;height:60px;margin-left:10px;max-width:40px;font-size:13px;}
.item_desc {display:block;float:left;line-height:60px;color:#666;margin-left:10px;font-size:12px;}
.item_time {display:block;float:right;line-height:60px;color:#999;margin-right:10px;font-size:11px;}
.item_dotted {position:absolute;top:10px;left:50px;background:red;width:4px;height:4px;border-radius:50%;}
