/* 签到 */

.qiandao_popup {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 30;
  animation: fadeIn 1s forwards;
  -webkit-animation: fadeIn 1s forwards;
}

.popup_bg {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
}

.popup_box {
  width: 660rpx;
  height: 860rpx;
  background-color: #fff;
  position: fixed;
  top: 10vh;
  left: 45rpx;
  border-radius: 36rpx;
}

.qiandao_word {
  width: 660rpx;
  height: 130rpx;
  background: #1eac58;
  border-top-left-radius: 36rpx;
  border-top-right-radius: 36rpx;
}

.sign_gift {
  width: 227rpx;
  height: 54rpx;
  position: absolute;
  top: 36rpx;
  left: 204rpx;
}

.close {
  color: #fff;
  font-size: 62rpx;
  position: absolute;
  right: 15rpx;
  top: 15rpx;
}

.rectangular_1 {
  width: 27rpx;
  height: 56rpx;
  position: absolute;
  top: 100rpx;
  left: 40rpx;
}

.rectangular_2 {
  width: 27rpx;
  height: 56rpx;
  position: absolute;
  top: 100rpx;
  right: 40rpx;
}

.words_box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding-bottom: 15rpx;
}

.continuous_days {
  font-size: 28rpx;
  color: #666;
  padding: 20rpx 0 6rpx 0;
  max-width: 650rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.question_box {
  display: flex;
  align-items: center;
}

.question {
  max-width: 620rpx;
  font-size: 40rpx;
  color: #ccc;
  margin-left: 10rpx;
}

.sign_begin {
  font-size: 24rpx;
  color: #999;
  overflow: hidden;
  max-width: 620rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 日历 */

.week_name {
  background-color: #f0f9f4;
  width: 100%;
  display: flex;
  padding: 20rpx 0;
}

.week_name view {
  flex: 1;
  text-align: center;
  font-size: 28rpx;
  color: #1eac58;
}

.week {
  width: 100%;
  padding: 10rpx 0;
  display: flex;
  flex-wrap: wrap;
}

.week view {
  /* width: 14.28%;
  height: 72rpx;
  line-height: 72rpx;
  display: inline-block;
  text-align: center; */
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  display: inline-block;
  text-align: center;
  padding-left: 3%;
}

.week view text {
  width: 100%;
  height: 100%;
  display: inline-block;
}

.submit {
  width: 307rpx;
  height: 90rpx;
  position: absolute;
  bottom: 25rpx;
  left: 163rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #1eac58;
  font-size: 40rpx;
  border: 2rpx solid #1eac58;
  border-radius: 45rpx;
}

.juanjiao {
  width: 144rpx;
  height: 170rpx;
  position: absolute;
  bottom: 0;
  right: 0;
}

/* 签到规则介绍弹窗 */

.signRule_popup {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: center;
  animation: fadeIn 1s forwards;
  -webkit-animation: fadeIn 1s forwards;
}

.signRulePopup_bg {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
}

.signRulePopup_box {
  width: 600rpx;
  height: 60vh;
  position: absolute;
  top: 20vh;
  left: 75rpx;
  background: linear-gradient(#b5e094, #57b567);
}

.rule_title {
  position: relative;
}

.left_line, .right_line {
  position: absolute;
  width: 166rpx;
  height: 2rpx;
  background: #fff;
  top: 60rpx;
}

.left_line {
  left: 34rpx;
}

.right_line {
  right: 34rpx;
}

.title_words {
  font-size: 38rpx;
  color: #fff;
  position: absolute;
  top: 36rpx;
  left: 225rpx;
}

.scroll_list {
  width: 100%;
  height: 41vh;
  position: absolute;
  top: 127rpx;
}

.rule_content {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.rule_content text {
  font-size: 30rpx;
  color: #fff;
  padding-bottom: 26rpx;
  width: 530rpx;
}

.close_2 {
  font-size: 76rpx;
  color: #fff;
  position: absolute;
  bottom: 0;
  left: 270rpx;
  margin: 10rpx 0;
}

/* 神秘大礼弹窗 */

.mysteriousGift_popup {
  width: 100%;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: center;
  animation: fadeIn 1s forwards;
  -webkit-animation: fadeIn 1s forwards;
}

.mystGift_bg {
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.4);
}

.mystGift_box {
  width: 448rpx;
  height: 378rpx;
  background: #f3f0f3;
  position: fixed;
  border-radius: 36rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.mystGift {
  width: 575rpx;
  height: 125rpx;
  position: absolute;
  top: -82rpx;
}

.mystGiftWords_box {
  width: 400rpx;
  height: 340rpx;
  background: #fff;
  border-radius: 36rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
}

.mystGift_words {
  width: 380rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #1eac58;
  line-height: 50rpx;
}

/* 启动页 */

.start_page {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
}

/* 底部导航 */

.simTabBar {
  width: 100%;
  height: 9vh;
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
}
