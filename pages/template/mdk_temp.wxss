.div image{
				width: 90px;
        height: 50px;
			}

.div{
  
  background-size: contain;
  background-repeat:no-repeat;
  position: fixed;
  margin: 0 auto;
  top:50%;
  left: 50%;
  width: 300px;
  height: 200px;
  margin: -100px 0 0 -150px;
  bottom:40px;
  z-index: 99999999;
}

.temp{
position: relative;

}

button.close::after, button.kf_button::after {
  content: none;
}

.close{
  position: absolute;
  background-color: transparent;
  text-align:center;
  border: 0;
  width: 20px;
  height: 20px;
  right:15px;
  top:21px;
  z-index: 0;
}

.kf_button{
  position: absolute;
  background-color: rgba(255, 255, 255, 0);
  text-align:center;
  top:140px;
  left:25px;
  width: 248px;
  height: 40px;
  
}


.hide{
  display: none;
}

.show{
  display: block;

}
