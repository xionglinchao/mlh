/* pages/readyClubAttorn/readyClubAttorn.wxss */
page {width:100%;height:100%;}
.pClass {width:100%;height:100%;background:#f0f0f0;}
.titleViewClass {width:calc(100% - 15px);height:40px;line-height:40px;font-size:15px;color:#333;margin-left:15px;border-bottom:1px solid #ddd;}
.type2 {width:calc(100% - 30px);margin-left:15px;height:40px;float:left;margin-top:10px;background:white;border-radius:4px;border:1px solid #ddd;}
.type2 .text2 {display:block;float:left;font-size:13px;line-height:40px;min-width:150px;height:40px;color:#666;margin-left:5px;}
.type2 image {width:20px;height:20px;margin-top:10px;margin-left:10px;float:right;margin-right:5px;}
.libpersonViewClass {width:calc(100% - 30px);margin-left:15px;background:white;float:left;}
.libpersonViewClass .scroll {width:100%;height:100%;}
.libpersonViewClass .textViewClass {width:100%;height:30px;border-bottom:1px solid #f0f0f0;}
.libpersonViewClass .textViewClass text {display:block;float:left;font-size:14px;color:#666;padding-left:10px;line-height:30px;}
.bottomViewClass {width:100%;height:44px;line-height:44px;text-align:center;font-size:14px;color:white;background:#1eac58;position:fixed;bottom:0px;border-radius:0px;}
.bottomViewClass::after {content:none;}
