
.maskBtn {
  width: 100%;
  background: transparent;
  padding: 0;
  margin: 0;
}

.maskBtn::after {
  border: none;
}

.homepage_container {
  width: 100%;
  height: 100vh;
  /* overflow: scroll; */
}

.hp_box {
  width: 100%;
  height: 100%;
  /* display: flex;
  flex-direction: column; */
}

/* 轮播图 */
.swiper_box {
  width: 690rpx;
  height: 200rpx;
  margin: 25rpx 30rpx;
}

.swiper_img {
  width: 690rpx;
  height: 200rpx;
  border-radius: 10rpx;
  background: lightblue;
}

/* 图标盒子 */
.icon_box {
  display: flex;
  justify-content: space-between;
  padding: 30rpx;
  border-bottom: 2rpx solid #F2F2F2;
}

.icon_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0;
  margin: 0;
  line-height: normal;
  background: transparent;
}

.icon_item::after {
  border: none;
}

.icon_img {
  width: 110rpx;
  height: 110rpx;
}

.icon_words {
  font-size: 24rpx;
  color: #545454;
  padding-top: 18rpx;
}

/* 社区动态 */
.community_container {
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #F2F2F2;
}

.community_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 26rpx 30rpx 10rpx 30rpx;
}

.sec_icon_box, .check_all_box {
  display: flex;
  align-items: center;
}

.community_icon {
  width: 26rpx;
  height: 30rpx;
}

.activ {
  width: 32rpx;
  height: 32rpx;
}

.community_words {
  font-size: 28rpx;
  padding-left: 18rpx;
}

.check_all {
  font-size: 28rpx;
  color: #AEAEAE;
}

.rightArrow {
  font-size: 26rpx;
  color: #AEAEAE;
  padding-left: 10rpx;
}

.community_cont_box {
  width: 730rpx;
  height: 264rpx;
  padding-left: 10rpx;
  vertical-align: top;
  position: relative;
}

.cont_container {
  width: 720rpx;
  height: 230rpx;
  position: absolute;
  top: 18rpx;
  z-index: 20;
  display: flex;
  justify-content: space-between;
}

.user_info_box {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 15rpx 0 0 30rpx;
}

.user_photo {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background: lightblue;
}

.user_name {
  font-size: 22rpx;
  max-width: 100rpx;
  padding-top: 10rpx;
}

.words_pic {
  display: flex;
  flex-direction: column;
  padding-top: 24rpx;
}

.cont_words {
  max-width: 410rpx;
  font-size: 28rpx;
  padding-bottom: 14rpx;
}

.cont_pic_box {
  width: 410rpx;
}

.cont_pic {
  width: 130rpx;
  height: 130rpx;
  vertical-align: top;
}

.cont_pic:nth-child(2n + 1) {
  margin: 0 4rpx;
}

.inter_box {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-end;
  padding-right: 16rpx;
}

.focus_btn {
  width: 80rpx;
  height: 40rpx;
  border-radius: 20rpx;
  background: #1EAC58;
  font-size: 20rpx;
  color: #FFFFFF;
  text-align: center;
  line-height: 40rpx;
  margin-top: 20rpx;
}

.like_message {
  display: flex;
  align-items: center;
  padding-bottom: 20rpx;
}

.like_empty {
  width: 30rpx;
  height: 28rpx;
}

.number {
  font-size: 26rpx;
  color: #B6B6B6;
  padding-left: 6rpx;
}

.msgIcon {
  font-size: 30rpx;
  color: #B6B6B6;
  padding-left: 6rpx;
}

/* 推荐课程 */
.select_course_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 690rpx;
  margin: 30rpx;
}

.courseIcon_box {
  display: flex;
  align-items: center;
}

.courseIcon {
  width: 35rpx;
  height: 33rpx;
  padding-right: 14rpx;
  color: #1EAC58;
}

.icon_word {
  font-size: 28rpx;
  color: #333333;
}

.select_course_list {
  width: 700rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  margin-left: 25rpx;
}

.course_show_box {
  width: 340rpx;
  height: 292rpx;
  border-radius: 12rpx;
  box-shadow:0rpx 5rpx 8rpx 0rpx rgba(0,0,0,0.19);
  margin-bottom: 22rpx;
}

.course_pic {
  width: 340rpx;
  height: 214rpx;
  border-radius:12rpx 12rpx 0px 0px;
  vertical-align: top;
  position: relative;
  background: lightblue;
}

.play_look_box {
  position: absolute;
  bottom: 0;
  z-index: 10;
  display: flex;
  width: 100%;
  background: rgba(0, 0, 0, 0.3)
}

.play_box {
  display: flex;
  align-items: center;
  padding: 6rpx 0;
}

.shipinIcon {
  width: 28rpx;
  height: 24rpx;
  padding-left: 16rpx;
}

.liuyan1Icon {
  color: #FFFFFF;
  font-size: 28rpx;
  padding: 4rpx 0 0 26rpx;
}

.play_num {
  font-size: 20rpx;
  color: #FFFFFF;
  padding-left: 10rpx;
}

.course_title {
  font-size: 24rpx;
  padding: 6rpx 6rpx 0 6rpx;
}

/* 推荐活动 */
.activ_list {
  display: flex;
  justify-content: space-between;
  margin: 0 30rpx;
}

.each_activ {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.activ_cover {
  width: 216rpx;
  height: 145rpx;
  background: lightblue;
  border-radius: 12rpx;
  position: relative;
}

.activ_cont_box {
  width: 216rpx;
  height: 145rpx;
  position: absolute;
  top: 0;
  z-index: 20;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-end;
}

.activ_on {
  width: 74rpx;
  height: 35rpx;
  margin-top: 4rpx;
}

.name_box {
  width: 200rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 8rpx 4rpx 8rpx;
  background: rgba(0, 0, 0, 0.4);
}

.activ_user_name {
  max-width: 90rpx;
  font-size: 22rpx;
  color: #FFFFFF;
}

.join_box {
  display: flex;
  align-items: center;
}

.people_icon {
  width: 26rpx;
  height: 23rpx;
}

.join_num {
  font-size: 20rpx;
  color: #FFFFFF;
  padding-left: 6rpx;
}

.activ_name {
  font-size: 30rpx;
  padding-top: 20rpx;
  max-width: 216rpx;
}

